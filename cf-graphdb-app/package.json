{
	"name": "cf-graphdb-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"type-check": "tsc --noEmit",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"dev:release": "npm run type-check && npm run test && npm run test:coverage && npm run deploy:dev",
		"ci:stage": "npm run type-check && npm run test && npm run test:coverage && npm run deploy:stage",
		"deploy:stage": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env stage",
		"deploy:dev": "opennextjs-cloudflare build && wrangler deploy --env dev",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"preinstall": "npx only-allow npm",
		"prepare": "npm run cf-typegen && npm run type-check"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.0.4",
		"next": "15.3.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.51",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/testing-library__jest-dom": "^5.14.9",
		"@vitejs/plugin-react": "^4.5.0",
		"@vitest/coverage-v8": "^3.1.4",
		"eslint": "^9",
		"eslint-config-next": "15.3.2",
		"jsdom": "^26.1.0",
		"tailwindcss": "^4",
		"typescript": "^5",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.4",
		"wrangler": "^4.17.0"
	}
}
